{
	"manifest_version": 2,
	"name": "Whater",
	"description": "A browser extension to visualize and interact with website to test responsiveness",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_title": "Whater"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"tabs"
	]
}
